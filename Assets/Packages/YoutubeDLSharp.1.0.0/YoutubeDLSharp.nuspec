<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>YoutubeDLSharp</id>
    <version>1.0.0</version>
    <authors>Bluegrams</authors>
    <license type="expression">BSD-3-Clause</license>
    <licenseUrl>https://licenses.nuget.org/BSD-3-Clause</licenseUrl>
    <readme>README.md</readme>
    <projectUrl>https://github.com/Bluegrams/YoutubeDLSharp</projectUrl>
    <description>A simple .NET wrapper library for youtube-dl and yt-dlp.

Note: Package versions &gt;= 1.0 are optimized to work with yt-dlp.
Package versions 0.x retain support for the original youtube-dl.</description>
    <releaseNotes>https://github.com/Bluegrams/YoutubeDLSharp/blob/master/Changelog.md</releaseNotes>
    <copyright>© 2020-2023 Bluegrams</copyright>
    <tags>youtube video download youtube-dl yt-dlp</tags>
    <dependencies>
      <group targetFramework=".NETFramework4.5">
        <dependency id="Newtonsoft.Json" version="13.0.3" exclude="Build,Analyzers" />
        <dependency id="System.ValueTuple" version="4.5.0" exclude="Build,Analyzers" />
      </group>
      <group targetFramework="net6.0">
        <dependency id="Newtonsoft.Json" version="13.0.3" exclude="Build,Analyzers" />
      </group>
      <group targetFramework=".NETStandard2.0">
        <dependency id="Newtonsoft.Json" version="13.0.3" exclude="Build,Analyzers" />
      </group>
    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System.IO.Compression" targetFramework=".NETFramework4.5" />
      <frameworkAssembly assemblyName="System.Net.Http" targetFramework=".NETFramework4.5" />
    </frameworkAssemblies>
  </metadata>
</package>